getter(name, type) ::= <<
<type> get<name; format="cap">();

>>

setter(name, type) ::= <<
void set<name; format="cap">(<type> <name>);

>>

field(name, type) ::= <<
private <type> <name>;

>>

getterImpl(name, type) ::= <<
@Override public <type> get<name; format="cap">() { return this.<name>; }

>>

setterImpl(name, type) ::= <<
@Override public void set<name; format="cap">(<type> <name>) { <setThis(name, type)> }

>>

setThis(name, type) ::= <%
this.<name> = <name>;
%>
